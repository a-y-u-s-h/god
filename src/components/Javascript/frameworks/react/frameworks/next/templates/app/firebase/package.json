    "yaml-loader": "^0.8.0"
  },
  "eslintConfig": {
    "extends": "next/core-web-vitals"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "@emotion/babel-plugin-jsx-pragmatic",
        {
          "module": "theme-ui",
          "import": "jsx",
          "export": "jsx"
        }
      ],
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragma": "jsx",
          "pragmaFrag": "React.Fragment"
        }
      ]
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": false,
    "trailingComma": "none",
    "tabWidth": 2,
    "printWidth": 1000,
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "requirePragma": false,
    "proseWrap": "preserve",
    "overrides": [
      {
        "files": "*.sass",
        "options": {
          "printWidth": 1000
        }
      },
      {
        "files": "*.mdx",
        "options": {
          "tabWidth": 2,
          "printWidth": 1000,
          "proseWrap": "always"
        }
      },
      {
        "files": "*.md",
        "options": {
          "tabWidth": 2,
          "printWidth": 1000,
          "proseWrap": "always",
          "htmlWhitespaceSensitivity": "strict"
        }
      },
      {
        "files": "*.html",
        "options": {
          "htmlWhitespaceSensitivity": "strict"
        }
      }
    ]
  }
}

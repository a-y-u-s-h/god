    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "react-icons": "^4.3.1",
    "react-is": "^18.1.0",
    "sass": "^1.52.1",
    "start-server-and-test": "^1.14.0",
    "storybook-addon-next": "^1.6.2",

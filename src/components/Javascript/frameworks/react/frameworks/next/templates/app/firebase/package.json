    "@mantine/prism": "^4.2.5",
    "@mantine/rte": "^4.2.5",
    "@mantine/spotlight": "^4.2.5",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.2",
    "@xstate/inspect": "^0.7.0",
    "@xstate/react": "^3.0.0",
    "animejs": "^3.2.1",
    "autoprefixer": "^10.4.7",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.2",
    "deepmerge": "^4.2.2",
    "firebase": "^9.8.1",
    "firebase-tools": "^11.0.0",
    "focus-visible": "^5.2.0",
    "framer-motion": "^6",
    "jest": "^28.1.0",
    "joi": "^17.6.0",
    "js-yaml": "^4.1.0",
    "js-yaml-loader": "^1.2.2",
    "mousetrap": "^1.6.5",
    "next": "^12.1.6",
    "next-connect": "^0.12.2",
    "p5": "^1.4.1",
    "postcss": "^8.4.14",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-hotkeys-hook": "^3.4.6",
    "rive-react": "^3.0.3",
    "tailwindcss": "^3.0.24",
    "tailwindcss-filters": "^3.0.0",
    "tone": "^14.7.77",
    "xstate": "^4.32.1",
    "yaml": "^2.1.0"

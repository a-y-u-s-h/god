    "@emotion/babel-plugin-jsx-pragmatic": "^0.1.5",
    "@storybook/addon-actions": "^6.5.4-alpha.0",
    "@storybook/addon-essentials": "^6.5.4-alpha.0",
    "@storybook/addon-knobs": "^6.3.1",
    "@storybook/addon-links": "^6.5.4-alpha.0",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storysource": "^6.5.4-alpha.0",
    "@storybook/builder-webpack5": "^6.5.4-alpha.0",
    "@storybook/manager-webpack5": "^6.5.4-alpha.0",
    "@storybook/react": "^6.5.4-alpha.0",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@theme-ui/presets": "^0.14.5",

{
  "name": "placeholder",
  "version": "1.0.0",
  "main": "src/index.js",
  "author": "Ayush Sharma <code.ayush@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prestart": "yarn clean",
    "start": "webpack --env.mode production && cross-env SKIP_PREFLIGHT_CHECK=true react-app-rewired start",
    "clean": "rimraf public build yarn.lock yarn-error.log package-lock.json",
    "execute": "cross-env SKIP_PREFLIGHT_CHECK=true react-app-rewired start",
    "test": "jest --config ./jest.config.js --passWithNoTests",
    "develop": "yarn start",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "update": "npx npm-check-updates -u --timeout 120000 && yarn install",
    "prepublish": "yarn test",
    "prebuild": "yarn clean",
    "build": "webpack --env.mode production && cross-env INLINE_RUNTIME_CHUNK=false SKIP_PREFLIGHT_CHECK=true react-app-rewired build && rimraf public && mv build public"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@types/chrome": "^0.0.120",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "customize-cra": "^1.0.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-parcel": "^1.10.4",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.7",
    "jest": "^26.1.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "react-app-rewire-alias": "^0.1.6",
    "react-app-rewired": "^2.1.6",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.0",
    "typescript": "^3.9.6",
    "webpack": "4.43.0",
    "webpack-cli": "^3.3.12",
    "yaml-loader": "^0.6.0"
  },
  "dependencies": {
    "framer-motion": "^1.11.1",
    "jquery": "^3.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-portal": "^4.2.1",
    "react-scripts": "^3.4.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": false,
    "trailingComma": "none",
    "tabWidth": 2,
    "arrowParens": "avoid",
    "printWidth": 200,
    "overrides": [
      {
        "files": "*.sass",
        "options": {
          "printWidth": 999
        }
      },
      {
        "files": "*.mdx",
        "options": {
          "tabWidth": 2,
          "printWidth": 80,
          "proseWrap": "always"
        }
      },
      {
        "files": "*.md",
        "options": {
          "tabWidth": 2,
          "printWidth": 80,
          "proseWrap": "always",
          "htmlWhitespaceSensitivity": "strict"
        }
      },
      {
        "files": "*.html",
        "options": {
          "htmlWhitespaceSensitivity": "strict"
        }
      }
    ]
  },
  "eslintConfig": {
    "extends": [
      "prettier"
    ],
    "plugins": [
      "react",
      "react-hooks"
    ],
    "rules": {
      "no-var": "error",
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "React"
        }
      ],
      "arrow-spacing": [
        "error",
        {
          "before": true,
          "after": true
        }
      ],
      "indent": [
        "error",
        2
      ],
      "semi": [
        "error",
        "never"
      ],
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "standard/object-curly-even-spacing": [
        "error",
        "either"
      ],
      "standard/array-bracket-even-spacing": [
        "error",
        "either"
      ],
      "standard/computed-property-even-spacing": [
        "error",
        "even"
      ],
      "standard/no-callback-literal": [
        "error",
        [
          "cb",
          "callback"
        ]
      ],
      "react/jsx-uses-react": "error",
      "react/jsx-uses-vars": "error",
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 8
    },
    "env": {
      "browser": true
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": true
          }
        }
      ]
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
